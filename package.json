    "@types/joi": "^17.2.3",
    "@types/milliseconds": "^0.0.30",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/sequelize": "^4.28.14",
    "babel-register": "^6.26.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@jest/types": "^29.4.1",
    "@nestjs/bull": "^0.6.2",
