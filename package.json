    "axios": "^1.2.4",
    "big.js": "^6.2.1",
    "bullmq": "^3.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi-browser": "^13.4.0",
