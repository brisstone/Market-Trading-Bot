{
  "name": "trading-bot",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --watch",
    "start:dev": "npx nodemon node",
    "clearDist": "npx rimraf dist",
    "build": "npm run clearDist && tsc -b",
    "build-watch": "npm run clearDist && npx tsc --watch",
    "start": "npm run build && nodemon dist/index.js",
    "kill:port": "npx kill-port 4000",
    "migrate:run": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "sequelize-cli migration:create  --name activity",
    "seed:run": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  }, 
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/big.js": "^6.1.6",
    "@types/express": "^4.17.15",
